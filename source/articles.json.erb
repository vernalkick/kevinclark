[
  <% blog.articles.each do |article| %>
    {
      "title": <%= article.title.to_json %>,
      "date": "<%= article.date %>",
      "filename": "<%= article.slug %>",
      "published": <%= article.data.published || "false" %>,
      "content": <%= article.summary.to_json %>
    }<%= "," unless article == blog.articles.last %>
  <% end %>
]
